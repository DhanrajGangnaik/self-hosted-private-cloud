{
  "acl": [
    {
      "action": "accept",
      "src": ["autogroup:members"],
      "dst": [
        "192.168.50.0/24:*"
      ]
    }
  ],
  "ssh": [
    {
      "action": "accept",
      "src": ["autogroup:members"],
      "dst": ["autogroup:self"],
      "users": ["root", "admin", "ubuntu"]
    }
  ],
  "nodeAttrs": [
    {
      "target": ["autogroup:members"],
      "attr": ["tag:homelab-access"]
    }
  ]
}